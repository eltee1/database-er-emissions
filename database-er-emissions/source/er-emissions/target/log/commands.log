 */ 
CREATE OR REPLACE VIEW check_totals_view AS
SELECT 
   emissiejaar,
   gcn_sector_id,
   er_stof_code,
   substance_id,
   gcn_stof_code,
   em_nt,
   em_reg,
   (em_nt - em_reg) AS verschil

   FROM 
      (SELECT 
         emissiejaar,
         er_stof_code,
         "left"(er_indeling_code, 4)::integer AS gcn_sector_id,
         round(emissie_kg::numeric, 3) AS em_nt
      
